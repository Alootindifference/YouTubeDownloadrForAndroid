<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 指定应该备份的数据 -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="file" path="temp/"/>
    </cloud-backup>
    <device-transfer>
        <!-- 指定在设备迁移时应该转移的数据 -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="file" path="temp/"/>
    </device-transfer>
</data-extraction-rules> 